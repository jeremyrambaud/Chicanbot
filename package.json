{
  "name": "GerardBot",
  "version": "1.0.0",
  "main": "dist/index.js",
  "author": "Zarwag",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "start:prod": "yarn node --experimental-specifier-resolution=node index.js",
    "start:dev": "tsc --build --watch & nodemon -w ./dist --inspect=0.0.0.0 --experimental-specifier-resolution=node ./dist/index.js",
    "deploy-commands": "yarn build && node --experimental-specifier-resolution=node ./dist/deploy-commands.js",
    "start": "npm run start:dev",
    "build": "tsc",
    "lint": "eslint src"
  },
  "dependencies": {
    "@discordjs/builders": "^0.11.0",
    "@discordjs/rest": "^0.2.0-canary.0",
    "bufferutil": "^4.0.6",
    "discord-api-types": "^0.26.1",
    "discord.js": "^13.6.0",
    "dotenv": "^14.1.0",
    "glob": "^7.2.0",
    "mongoose": "^6.1.7",
    "ts-node": "^10.4.0",
    "utf-8-validate": "^5.0.8",
    "zlib-sync": "^0.1.7"
  },
  "packageManager": "yarn@3.1.1",
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/node": "^17.0.9",
    "nodemon": "^2.0.15",
    "tsc-watch": "^4.6.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  }
}
